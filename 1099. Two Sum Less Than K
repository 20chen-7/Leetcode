class Solution:
    def twoSumLessThanK(self, nums: List[int], k: int) -> int:
        res = -float('inf')
        for i in range(len(nums)):
            for j in range(i+1, len(nums)):
                sums = nums[i] + nums[j]
                if sums < k:
                    res = max(res, sums)
        if res == -float('inf'):
            return -1
        return res
