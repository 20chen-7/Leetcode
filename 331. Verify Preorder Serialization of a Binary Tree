class Solution:
    def isValidSerialization(self, preorder: str) -> bool:
        stack = []
        for node in preorder.split(','):
            stack.append(node)
            while len(stack) >= 3 and stack[-1]== stack[-2]== '#' and stack[-3]!='#':
                stack.pop(), stack.pop(), stack.pop()
                stack.append('#')
        if len(stack) == 1:
            if stack.pop() == '#':
                return True
        return False
